{
  "name": "Social Interceptor",
  "version": "1.0",
  "manifest_version": 2,
  "description": "tk",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["find_buttons.js"],
      "run_at": "document_end"
    }
  ]
}
